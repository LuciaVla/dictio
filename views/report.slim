              a.btn class="btn-secondary report-" onclick="URLAppend('translationogs')" ÖGS
          div
            | #{I18n.t("admin.pubtrans")}:
            - unless @params['pubtranscs']                       
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtranscs')" CS
            - unless @params['pubtransczj']
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtransczj')" ČZJ            
            - unless @params['pubtransen']
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtransen')" EN              
            - unless @params['pubtransasl']
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtransasl')" ASL
            - unless @params['pubtransis']
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtransis')" IS
            - unless @params['pubtransj']
              a.btn class="btn-secondary report-" onclick="URLAppend('pubtranssj')" SJ
            - unless @params['pubtransspj']
