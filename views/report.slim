                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','modif','sldruh','ne')" #{I18n.t("gram.slovni_druh.modif")}
              - unless @params['sldruh'] and @params['slovni_druh'] == 'part'          
                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','part','sldruh','ne')" #{I18n.t("gram.slovni_druh.part")}              
              - unless @params['sldruh'] and @params['slovni_druh'] == 'konj'                      
                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','konj','sldruh','ne')" #{I18n.t("gram.slovni_druh.konj")}
              - unless @params['sldruh'] and @params['slovni_druh'] == 'kat'
                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','kat','sldruh','ne')" #{I18n.t("gram.slovni_druh.kat")}                          
              - unless @params['sldruh'] and @params['slovni_druh'] == 'klf'            
                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','klf','sldruh','ne')" #{I18n.t("gram.slovni_druh.klf")}
              - unless @params['sldruh'] and @params['slovni_druh'] == 'spc'
                a.btn class="btn-secondary report-" onclick="URLChange2('slovni_druh','spc','sldruh','ne')" #{I18n.t("gram.slovni_druh.spc")}                                                                                             
          div
            | #{I18n.t("admin.sem")}:  
            - unless @params['vyznamvid']
              a.btn class="btn-secondary report-" onclick="URLAppend('vyznamvid')" #{I18n.t("admin.vyznamvid")}
            - unless @params['vyznam']
              a.btn class="btn-secondary report-" onclick="URLAppend('vyznam')" #{I18n.t("admin.vyznam")}    
          div
            | #{I18n.t("admin.ekv")}: 
            | #{I18n.t("admin.translation")}:               
            - unless @params['translationcs']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationcs')" CS              
            - unless @params['translationczj']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationczj')" ČZJ              
            - unless @params['translationen']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationen')" EN
            - unless @params['translationasl']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationasl')" ASL
            - unless @params['translationis']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationis')" IS
            - unless @params['translationsj']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationsj')" SJ
            - unless @params['translationspj']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationspj')" SPJ
            - unless @params['translationde']
              a.btn class="btn-secondary report-" onclick="URLAppend('translationde')" DE                            
